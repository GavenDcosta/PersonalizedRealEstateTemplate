{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/auth/(.*)", "dest": "/api/auth/$1" },
      { "src": "/api/users/(.*)", "dest": "/api/users/$1" },
      { "src": "/api/posts/(.*)", "dest": "/api/posts/$1" },
      { "src": "/api/test/(.*)", "dest": "/api/test/$1" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  